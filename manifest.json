{
  "name": "WhitePage",
  "description": "A Custom simple start page. It opens the \"downloads\" page when you click on it.",
  "version": "1.0",
  "author":"Ivan Pillay",
  "browser_action": {	
      "default_icon": "download-icon.png",
      "default_title": "Downloads"
  },
  "icons": {"256":"icon.png"},
  "incognito": "split",
  "chrome_url_overrides": {
		"newtab": "index.html"
  },
  "background":{
	"scripts": ["download.js"]
   },
  "manifest_version": 2
 }